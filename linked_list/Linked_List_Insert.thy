  s' = s
  \<and> n \<noteq> NULL
  \<and> is_valid_ll_C s n
  \<and> s[n]\<rightarrow>next = NULL
  \<and> list (lkup s) xs' i 
  \<and> n \<notin> set xs'\<rbrace>
  insert' n i
\<lbrace> \<lambda> r s. (\<exists> xs . list (lkup s) xs r \<and> set (n # xs') = set xs)\<rbrace>!"
 apply (rule validNF_assume_pre)
