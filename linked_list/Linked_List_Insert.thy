      apply (auto intro: list_all_valid d)[1]
     apply (rule else_branch)
      apply (simp add: list_member_not_null) 
     apply (auto intro: list_all_valid e)[1]
    apply (auto intro: list_all_valid insert_front)[1]
   apply (auto intro: list_all_valid insert_front)[1]
